				    .append('<option value="2">3 Months</option>')
				    .append('<option value="3">6 Months</option>')
				    .append('<option value="4">12 Months</option>')
				;
            }else if(time_option_string == "Year"){  
            	document.getElementById("pastData").style.display = ""; 
                 document.getElementById("table_search").style.display = "";
            	$('#time_category_sub_select')
				    .find('option')
				    .remove()
				    .end()
				    .append('<option value="1">1 Year</option>')
				    .append('<option value="2">2 Years</option>')
				    .append('<option value="3">3 Years</option>')
				;        	
            }else{
            	document.getElementById("pastData").style.display = "none";
                document.getElementById("table_search").style.display = "none";
            }


        };

