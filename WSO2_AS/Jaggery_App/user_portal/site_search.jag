    function getOptionValue() {

            var option_value = document.getElementById("category_select");
            var option_string = option_value.options[option_value.selectedIndex].text;
            document.getElementById("change_text").innerHTML = option_string;
            if(option_string == "Site Name"){
                document.getElementById("change_text").innerHTML = option_string;
                document.getElementById("time_category_table").style.display = "";  

            }else if(option_string == "Site IP"){
                document.getElementById("change_text").innerHTML = option_string;
                document.getElementById("time_category_table").style.display = "";

            }else{
                document.getElementById("change_text").innerHTML = ""; 
                document.getElementById("time_category_table").style.display = "none";
                document.getElementById("table_search").style.display = "none";

            }
        };

        function getTimeOptionValue(){

        	var time_option_value = document.getElementById("time_category_select");
            var time_option_string = time_option_value.options[time_option_value.selectedIndex].text;
            if (time_option_string == "Current"){
            	document.getElementById("pastData").style.display = "none";                               
                document.getElementById("table_search").style.display = "";
            }else if(time_option_string == "Hour"){    
            	//select.options.length = 0;        	
            	document.getElementById("pastData").style.display = "";                               
                document.getElementById("table_search").style.display = "";
