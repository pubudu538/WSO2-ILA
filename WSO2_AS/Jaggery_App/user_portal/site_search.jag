				    .end()
				    .append('<option value="1">1 Hour</option>')
				    .append('<option value="2">3 Hours</option>')
				    .append('<option value="3">6 Hours</option>')
				    .append('<option value="4">12 Hours</option>')
				    .append('<option value="5">24 Hours</option>')
				;
            }else if(time_option_string == "Day"){
            	document.getElementById("pastData").style.display = "";                                  
                document.getElementById("table_search").style.display = "";    
            	$('#time_category_sub_select')
				    .find('option')
				    .remove()
				    .end()
				    .append('<option value="1">1 Day</option>')
				    .append('<option value="2">5 Days</option>')
				    .append('<option value="3">10 Days</option>')
				    .append('<option value="4">15 Days</option>')
				    .append('<option value="5">20 Days</option>')
				    .append('<option value="6">25 Days</option>')
				    .append('<option value="7">30 Days</option>')
				;
            }else if(time_option_string == "Month"){
            	document.getElementById("pastData").style.display = "";                                
