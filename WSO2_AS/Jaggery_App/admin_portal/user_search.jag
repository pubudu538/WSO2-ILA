                            } );

                		counter++;

                    }else if(option_select_string=="IP Address"){
                          
                          
                          if(counter>0){
			                	//alert(counter);
			                	oTable.fnClearTable();
			                	oTable.fnReloadAjax('functions/usersearch_ip.jag?ip_address='+searchBoxValue);
			                	counter = 0;
			                }

                           var ip_address = searchBoxValue;
                
                           oTable =  ggh('#example').dataTable( {
