                    }else if(option_select_string=="MAC Address"){

                    		if(counter>0){
			                	//alert(counter);
			                	oTable.fnClearTable();
			                	oTable.fnReloadAjax('functions/usersearch_mac.jag?mac_address='+searchBoxValue);
			                	counter = 0;
