    function getOptionValue() {

            var option_value = document.getElementById("category_select");
            var option_string = option_value.options[option_value.selectedIndex].text;

            if (option_string == "User Name"){

                document.getElementById("change_text").innerHTML = option_string;
                document.getElementById("site_table").style.display = "";

                document.getElementById("tableHeader1").innerHTML = "Full Name";
                document.getElementById("tableHeader2").innerHTML = "MAC Address";
                document.getElementById("tableHeader3").innerHTML = "IP Address";

                createTable(option_string);

            }else if(option_string == "IP Address"){

                document.getElementById("change_text").innerHTML = option_string;
                document.getElementById("site_table").style.display = "";

                document.getElementById("tableHeader1").innerHTML = "IP Address";
                document.getElementById("tableHeader2").innerHTML = "Full Name";
                document.getElementById("tableHeader3").innerHTML = "MAC Address";

                createTable(option_string);

            }else if(option_string == "MAC Address"){

                document.getElementById("change_text").innerHTML = option_string;             
                document.getElementById("site_table").style.display = "";

                document.getElementById("tableHeader1").innerHTML = "MAC Address";
                document.getElementById("tableHeader2").innerHTML = "Full Name";
                document.getElementById("tableHeader3").innerHTML = "IP Address";

                createTable(option_string);

            }else{

                document.getElementById("change_text").innerHTML = "";                
                document.getElementById("site_table").style.display = "none";

            }
        };

    function getCategory(){

	            var option_value = document.getElementById("category_select");
	            var option_string = option_value.options[option_value.selectedIndex].text;
	            document.getElementById("change_text").innerHTML = option_string;

            if (option_string == "-- Select a Category --"){
                document.getElementById("change_text").innerHTML = "";
                document.getElementById("secondDropDown").style.display = "none";
            }else{
                document.getElementById("secondDropDown").style.display = "";
            }



        };


    function deleteConfirm(delId){

    		//alert("One : "+delId);

                $( "#dialog-confirm" ).dialog({
                    resizable: false,
                    height:140,
                    modal: true,
                    buttons: {
                        "Delete This Data": function() {
                            deleteData(delId);
                            $( this ).dialog( "close" );
                        },
                        Cancel: function() {
                            $( this ).dialog( "close" );
                        }
                    }
                });


    };

    function deleteData(idToDelete){

            //alert("Two :"+idToDelete);

                $.ajax({
                      url: "functions/deleteUser.jag",
                      context: document.body,
                      data: {"delete_id": idToDelete}
                    }).done(function(data) {
                        alert("Data Deleted Succesfuly");
                        if(!data.error) location.reload(true);
                    }).fail(function() {
                        alert("Fail");
                      });

        };


    
