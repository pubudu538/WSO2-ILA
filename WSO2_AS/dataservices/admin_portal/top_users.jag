                                    },
                                    "bRetrieve": true,"sDom": 'T<"clear">lfrtip',
                                    "oTableTools": {
                                        "sSwfPath": "media/swf/copy_csv_xls_pdf.swf"
                                    },
                                    "bDestroy": true,
                                    "sAjaxSource": "functions/current_topusers.jag"
                            } );

                            triggerCounter++;

                    }else if(category_type=="Day"){
                          
                          if(triggerCounter > 0){
