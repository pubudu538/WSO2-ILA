                                    },
                                    "bRetrieve": true,"sDom": 'T<"clear">lfrtip',
                                    "oTableTools": {
                                        "sSwfPath": "media/swf/copy_csv_xls_pdf.swf"
                                    },
                                        "bDestroy": true,
                                        "sAjaxSource": "functions/daily_topusers.jag"
                                } );

                               triggerCounter++;

                    }else if(category_type=="Month"){
                          
                          if(triggerCounter > 0){
                             oTable.fnDestroy();
                        }

                               oTable =  ggh('#example').dataTable( {
                                        "sPaginationType": "full_numbers",
                                        "bProcessing": true,
                                        "bRetrieve": true,
                                    "oLanguage": {
                                     "sZeroRecords": "No Matching Record Found"
                                    },
                                    "fnInitComplete": function ( oSettings ) {
                                      oSettings.oLanguage.sZeroRecords = "No matching records found"
                                    },
                                    "bRetrieve": true,"sDom": 'T<"clear">lfrtip',
                                    "oTableTools": {
                                        "sSwfPath": "media/swf/copy_csv_xls_pdf.swf"
                                    },
                                        "bDestroy": true,
                                        "sAjaxSource": "functions/monthly_topusers.jag"
                                } ); 

                                triggerCounter++;            

                    }else if(category_type=="Year"){
                        
                           if(triggerCounter > 0){
                             oTable.fnDestroy();
                        }

                               oTable =  ggh('#example').dataTable( {
                                        "sPaginationType": "full_numbers",
                                        "bProcessing": true,
                                        "bRetrieve": true,
                                    "oLanguage": {
